{
  "name": "@swimlane/prettier-config-swimlane",
  "version": "3.0.0",
  "description": "Prettier configuration used in Swimlane projects.",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test",
    "doc": "docs"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "jasmine JASMINE_CONFIG_PATH=test/support/jasmine.json",
    "lint": "eslint .",
    "prettier": "prettier --write \"**/*.js\"",
    "preversion": "npm run lint && npm test",
    "postversion": "git push && git push --tags",
    "np": "np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimlane/prettier-config-swimlane.git"
  },
  "keywords": [
    "swimlane",
    "prettier",
    "config",
    "configuration",
    "style",
    "guide"
  ],
  "author": "Swimlane <contact@swimlane.com> (https://github.com/swimlane)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swimlane/prettier-config-swimlane/issues"
  },
  "homepage": "https://github.com/swimlane/prettier-config-swimlane#readme",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-swimlane": "^1.1.0",
    "jasmine": "^3.1.0",
    "jsonschema": "^1.2.4",
    "np": "^6.2.0",
    "prettier": "^2.0.4"
  },
  "peerDependencies": {
    "prettier": "^2.0.4"
  }
}
